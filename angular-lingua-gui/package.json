{
  "name": "angular-lingua-gui",
  "version": "0.1.6",
  "description": "The web based management interface for your angular-lingua translations",
  "keywords": [
    "@angular",
    "angular",
    "angular-lingua",
    "GUI",
    "i18n"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/css-ch/angular-lingua.git"
  },
  "license": "AGPL",
  "author": "Donato Wolfisberg <donato.wolfisberg@css.ch>",
  "bin": {
    "angular-lingua-gui": "dist/index.js"
  },
  "scripts": {
    "build": "npm run build-backend && npm run build-frontend",
    "build-backend": "tsc -p ./backend",
    "build-frontend": "ng build --prod",
    "ng": "ng",
    "npm:publish:patch": "npm run build && npm version patch && npm publish",
    "start": "npm run build-frontend && npm run start-backend",
    "start-backend": "ts-node --project ./backend/tsconfig.json ./backend/src/index.ts --browser=false ./test-data/locales.ts deu ita fra",
    "start-frontend": "ng serve",
    "ts-node": "ts-node",
    "tsc": "tsc",
    "watch-backend": "nodemon -e ts --watch backend --exec npm run start-backend"
  },
  "dependencies": {
    "chokidar": "^3.0.1",
    "commander": "^2.20.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "get-port": "^5.0.0",
    "open": "^6.3.0",
    "require-from-string": "^2.0.2",
    "typescript": "~3.4.5",
    "ws": "^7.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.800.2",
    "@angular/animations": "^8.0.0",
    "@angular/cdk": "^8.0.0",
    "@angular/cdk-experimental": "^8.0.0",
    "@angular/cli": "^8.0.2",
    "@angular/common": "^8.0.0",
    "@angular/compiler": "^8.0.0",
    "@angular/compiler-cli": "^8.0.0",
    "@angular/core": "^8.0.0",
    "@angular/forms": "^8.0.0",
    "@angular/http": "^7.2.15",
    "@angular/language-service": "^8.0.0",
    "@angular/material": "^8.0.0",
    "@angular/platform-browser": "^8.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "@angular/router": "^8.0.0",
    "@types/chokidar": "^2.1.3",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.0",
    "@types/get-port": "^4.2.0",
    "@types/jasmine": "^3.3.13",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^12.0.7",
    "@types/require-from-string": "^1.2.0",
    "@types/ws": "^6.0.1",
    "codelyzer": "~5.1.0",
    "core-js": "^3.1.3",
    "fuse.js": "^3.4.5",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "protractor": "^5.4.2",
    "rxjs": "~6.5.2",
    "ts-node": "^8.2.0",
    "tslib": "^1.9.3",
    "tslint": "~5.17.0",
    "webpack-dev-server": "^3.7.0",
    "zone.js": "~0.9.1"
  }
}
